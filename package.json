{
  "name": "playwright-report-parser",
  "version": "0.0.0-semantically-released",
  "description": "A lightweight TypeScript library to programmatically parse and analyze Playwright HTML test reports.",
  "license": "ISC",
  "author": "Mark Skelton <mark@mskelton.dev>",
  "repository": "https://github.com/mskelton/playwright-report-parser",
  "bin": "./dist/cli.js",
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "format": "oxfmt .",
    "lint": "eslint .",
    "format:check": "oxfmt --check .",
    "typecheck": "tsc --noEmit",
    "test": "playwright test"
  },
  "dependencies": {
    "adm-zip": "^0.5.16"
  },
  "devDependencies": {
    "@mskelton/eslint-config": "^9.3.0",
    "@playwright/test": "^1.48.2",
    "@types/adm-zip": "^0.5.7",
    "@types/node": "^22.7.9",
    "dedent": "^1.7.1",
    "eslint": "^10.0.0",
    "execa": "^9.5.2",
    "oxfmt": "^0.33.0",
    "semantic-release": "^25.0.3",
    "typescript": "^5.6.3"
  },
  "packageManager": "yarn@4.12.0"
}
