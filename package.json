{
  "name": "playwright-report-parser",
  "description": "A lightweight TypeScript library to programmatically parse and analyze Playwright HTML test reports.",
  "version": "0.0.0-semantically-released",
  "repository": "https://github.com/mskelton/playwright-report-parser",
  "author": "Mark Skelton <mark@mskelton.dev>",
  "license": "ISC",
  "packageManager": "yarn@4.12.0",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "test": "playwright test"
  },
  "dependencies": {
    "adm-zip": "^0.5.16"
  },
  "devDependencies": {
    "@anolilab/semantic-release-clean-package-json": "^4.2.2",
    "@mskelton/semantic-release-config": "^1.0.1",
    "@playwright/test": "^1.48.2",
    "@types/adm-zip": "^0.5.7",
    "@types/node": "^22.7.9",
    "dedent": "^1.7.1",
    "execa": "^9.5.2",
    "prettier": "^3.6.2",
    "semantic-release": "^25.0.2",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  }
}
